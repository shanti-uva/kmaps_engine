<% header_options = {class: 'html not-front logged-in no-sidebars page-subjects kmaps', javascript_files: ['kmaps_engine/admin'], stylesheet_files: ['kmaps_engine/admin']}
   header_options[:title] = "#{model_name.classify.constantize.model_name.human.titleize}: #{controller.action_name.humanize}" if defined?(model_name) && !model_name.blank? %>
<%= header(header_options) %>
<!-- Banner Region -->
<main class="main-wrapper container">
	<article class="main-content" role="main">
		<div class="row">
			<!-- Banner Region -->
			<header class="col-xs-12 titlearea banner">
				<div role="banner">
					<h1 class="page-title"><span class="icon shanticon-subjects"></span><span class="page-title-text"><%= @page_title if @page_title %></span></h1>
					<nav class="breadwrap">
<%=                   render_breadcrumbs %>
					</nav>
					<div class="banner-content"></div>
					<div class="banner-tabs"></div>
				</div>
			</header>
		</div> <!-- End of Banner Region -->
		<!-- Begin Content Row -->
		<div class="row row-offcanvas row-offcanvas-left">
			<!-- Sidebar First Region -->
            <div class="right"><%= resource_nav %></div>
			<!-- Begin Page Content -->
			<section class="content-section">
				<button type="button" class="btn btn-default view-resources" data-toggle="offcanvas" style="display:none;">
					<span class="icon"></span>
				</button>
	          	<!-- Message Area -->
	          	<!-- Main Content -->
				<div class="region region-content">
					<div id="block-system-main" class="block block-system">
						<div class="content">
<%=                       yield %>
						</div>
					</div>
				</div>
			</section>
			<!-- END Content -->
			<!-- Sidebar Second Region -->
		</div>
	</article>
</main>
<%= render 'public/admin_menu' %>
<%= footer %>